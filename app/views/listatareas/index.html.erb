<p id="notice"><%= notice %></p>

<h1>Listatareas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>F desde</th>
      <th>F hasta</th>
      <th>Responsable</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listatareas.each do |listatarea| %>
      <tr>
        <td><%= listatarea.nombre %></td>
        <td><%= listatarea.f_desde %></td>
        <td><%= listatarea.f_hasta %></td>
        <td><%= listatarea.responsable %></td>
        <td><%= listatarea.estado %></td>
        <td><%= link_to 'Show', listatarea %></td>
        <td><%= link_to 'Edit', edit_listatarea_path(listatarea) %></td>
        <td><%= link_to 'Destroy', listatarea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listatarea', new_listatarea_path %>
